{
  "kind": "collectionType",
  "collectionName": "invoices",
  "info": {
    "name": "Invoice",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "FA",
        "VD"
      ]
    },
    "date": {
      "type": "datetime"
    },
    "vat": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal"
    },
    "salles_man": {
      "via": "invoices",
      "model": "salles-man"
    },
    "invoice_items": {
      "via": "invoice",
      "collection": "invoice-item"
    },
    "customer": {
      "model": "customers",
      "via": "invoices"
    },
    "items": {
      "type": "component",
      "repeatable": true,
      "component": "items.invoice-item"
    }
  }
}
